<td colspan="2" style="padding-top:8px" onmouseover="clearTimeout(nt.t);nt.f=1;" onmouseout="if(nt.f)nt.t=setTimeout('nt.hidden();',1500);"><div id="notice"></div>
<script language="javascript">
function bbsNotice(){
	this.nt = ['<a href="notice.php?fid=-1#2">زىيارەتچىلەر دىقىتىگە </a> <span class="f9 gray">08-02-28</span>','<a href="notice.php?fid=-1#1">زىيارەتچىلەر سەمىگە </a> <span class="f9 gray">07-12-29</span>'];this.i = this.t = this.w = this.f = 0;this.v = [0,3,8,20,35,62,100];
	this.n = parseInt('2');
	this.play = function(){
		var j = this.i+this.n>this.nt.length ? this.nt.length : this.i+this.n;
		var s = '';
		for(var p=this.i;p<j;p++) s += this.nt[p];
		getObj('notice').innerHTML = s;
		if(this.n < this.nt.length && !this.f){
			this.t = setTimeout("nt.hidden();",4000);
		}
	}
	this.hidden = function(){
		this.f=0;
		this.i+=this.n;
		if(this.i>=this.nt.length){
			this.i=0;
		}
		getObj('notice').innerHTML = '';
		setTimeout("nt.play();",850);
	}
	this.play();
}
var nt = new bbsNotice();
</script>
