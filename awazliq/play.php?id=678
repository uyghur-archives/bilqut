byid = document.getElementById;

//表单提交
function submit_form(){
var postuser = document.reply.user.value;
var postcotent = document.reply.content.value;

if (postuser=="" || postcotent=="" )
		{
			alert("请填写所有项目");
			return false; 
		  }
document.reply.submit();
document.reply.content.value = '';
document.reply.user.value = '';
byid("post").style.display = 'none';
byid("msg").innerHTML='<font color=red>باھا يوللاش تامام</font>';
}

</script>

