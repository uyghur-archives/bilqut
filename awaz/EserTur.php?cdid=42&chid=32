	function ShowThis(idname,id)
	{
		if(id==1)
		{
			document.all[idname+id].style.display='block';
			document.all[idname+'2'].style.display='none';								
		}
		if(id==2)
		{
			document.all[idname+id].style.display='block';
			document.all[idname+'1'].style.display='none';						
		}
	}	
</script>
